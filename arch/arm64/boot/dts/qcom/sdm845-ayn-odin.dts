/dts-v1/;

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/pinctrl/qcom,pmic-gpio.h>
#include <dt-bindings/regulator/qcom,rpmh-regulator.h>
#include <dt-bindings/input/linux-event-codes.h>
#include "sdm845.dtsi"

// Needed for some GPIO (like the volume buttons)
#include "pm8998.dtsi"
#include "pmi8998.dtsi"
#include "pm8005.dtsi"

/ {
	model = "AYN Odin";
	compatible = "qcom,sda845", "qcom,sdm845", "qcom,sdm845-mtp";
	/* required for bootloader to select correct board. 
	 * It might not be needed tho, since we're usually using UEFI boot
	 */
	qcom,board-id = <0x08 0x00>;
	qcom,msm-id = <0x155 0x20001>;

	aliases {
		serial0 = &uart9;
		hsuart0 = &uart6;
	};

	chosen {
		#address-cells = <2>;
		#size-cells = <2>;
		ranges;

		// For simplefb hack
		stdout-path = "display0";

		/* hack: use framebuffer setup by bootloader.
		 * the address is taken from the bootloader config (strings xbl.img | grep "Display Reserved")
		 */
		framebuffer0: framebuffer@9d400000 {
			compatible = "simple-framebuffer";
			reg = <0 0x9D400000 0 0x02400000>;
			width = <1080>;
			height = <1920>;
			stride = <(1080 * 4)>;
			format = "a8r8g8b8";
			status = "okay";
		};
	};

	vph_pwr: vph-pwr-regulator {
		compatible = "regulator-fixed";
		regulator-name = "vph_pwr";
		regulator-min-microvolt = <3700000>;
		regulator-max-microvolt = <3700000>;
	};
	
	/*
	 * Apparently RPMh does not provide support for PM8998 S4 because it
	 * is always-on; model it as a fixed regulator.
	 */
	vreg_s4a_1p8: pm8998-smps4 {
		compatible = "regulator-fixed";
		regulator-name = "vreg_s4a_1p8";
		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <1800000>;
		regulator-always-on;
		regulator-boot-on;
		vin-supply = <&vph_pwr>;
	};

	hdmi-connector {
		compatible = "hdmi-connector";
		label = "hdmi";
		type = "d";

		port {
			hdmi_connector_in: endpoint {
				remote-endpoint = <&lt8912_out>;
			};
		};
	};

	gpio_keys {
		compatible = "gpio-keys";
		autorepeat;

		pinctrl-names = "default";
		pinctrl-0 = <&vol_up_pin_a>;

		vol-up {
			label = "Volume Up";
			linux,code = <KEY_VOLUMEUP>;
			gpios = <&pm8998_gpio 6 GPIO_ACTIVE_LOW>;
		};

		home {
			label = "HOME";
			linux,code = <KEY_HOME>;
			gpios = <&tlmm 49 GPIO_ACTIVE_LOW>;
		};

		select {
			label = "SELECT";
			linux,code = <BTN_SELECT>;
			gpios = <&tlmm 0 GPIO_ACTIVE_LOW>;
		};

		start {
			label = "START";
			linux,code = <BTN_START>;
			gpios = <&tlmm 3 GPIO_ACTIVE_LOW>;
		};

		up {
			label = "UP";
			linux,code = <BTN_DPAD_UP>;
			gpios = <&tlmm 124 GPIO_ACTIVE_LOW>;
		};

		down {
			label = "DOWN";
			linux,code = <BTN_DPAD_DOWN>;
			gpios = <&tlmm 128 GPIO_ACTIVE_LOW>;
		};

		left {
			label = "LEFT";
			linux,code = <BTN_DPAD_LEFT>;
			gpios = <&tlmm 52 GPIO_ACTIVE_LOW>;
		};

		right {
			label = "RIGHT";
			linux,code = <BTN_DPAD_RIGHT>;
			gpios = <&tlmm 122 GPIO_ACTIVE_LOW>;
		};

		x {
			label = "Button X";
			linux,code = <BTN_X>;
			gpios = <&tlmm 92 GPIO_ACTIVE_LOW>;
		};

		y {
			label = "Button Y";
			linux,code = <BTN_Y>;
			gpios = <&tlmm 41 GPIO_ACTIVE_LOW>;
		};

		a {
			label = "Button A";
			linux,code = <BTN_A>;
			gpios = <&tlmm 96 GPIO_ACTIVE_LOW>;
		};

		b {
			label = "Button B";
			linux,code = <BTN_B>;
			gpios = <&tlmm 95 GPIO_ACTIVE_LOW>;
		};

		r {
			label = "Button R";
			linux,code = <BTN_TR>;
			gpios = <&tlmm 91 GPIO_ACTIVE_LOW>;
		};

		l {
			label = "Button L";
			linux,code = <BTN_TL>;
			gpios = <&tlmm 26 GPIO_ACTIVE_LOW>;
		};
	};
};


